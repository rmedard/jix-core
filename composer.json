{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "ext-ftp": "*",
        "composer/installers": "^1.9",
        "drupal/addtoany": "^1.14",
        "drupal/admin_toolbar": "^2.3",
        "drupal/adminimal_admin_toolbar": "^1.11",
        "drupal/adsense": "^1.2",
        "drupal/back_to_top": "^1.1",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/bootstrap_sass": "^5.0",
        "drupal/clientside_validation": "^3.0@RC",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/core-recommended": "^9",
        "drupal/country": "1.x-dev@dev",
        "drupal/date_popup": "^1.1",
        "drupal/delete_all": "^2.0",
        "drupal/devel": "^4.0",
        "drupal/editor_advanced_link": "^1.8",
        "drupal/field_permissions": "^1.1",
        "drupal/filefield_paths": "1.x-dev@dev",
        "drupal/fivestar": "^1.0@alpha",
        "drupal/fontawesome": "^2.17",
        "drupal/geotimezone": "^3.0",
        "drupal/google_analytics": "4.x-dev@dev",
        "drupal/honeypot": "^2.0",
        "drupal/imagemagick": "^3.2",
        "drupal/jquery_ui_effects": "^1.2",
        "drupal/jsonapi_extras": "^3.17",
        "drupal/memcache": "^2.3",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/module_filter": "3.x-dev",
        "drupal/pathauto": "^1.8",
        "drupal/publication_date": "^2.0@beta",
        "drupal/restui": "^1.19",
        "drupal/rules": "3.x-dev@dev",
        "drupal/simplenews": "^3.0@alpha",
        "drupal/smart_ip": "^4.0",
        "drupal/swiftmailer": "^2.0@beta",
        "drupal/telephone_validation": "^2.3",
        "drupal/token": "^1.7",
        "drupal/twig_tweak": "^3.0",
        "drupal/views_autocomplete_filters": "^1.3",
        "drupal/views_bootstrap": "4.x-dev@dev",
        "drupal/views_data_export": "^1.0",
        "drupal/webform": "^6.0",
        "drupal/webform_views": "^5.0@alpha",
        "drush/drush": "^10.3",
        "kint-php/kint": "^3.3",
        "phpseclib/phpseclib": "^3.0"
    },
    "require-dev": {
        "drupal/core-dev": "^9"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "httpdocs/"
            }
        },
        "installer-paths": {
            "httpdocs/core": [
                "type:drupal-core"
            ],
            "httpdocs/libraries/{$name}": [
                "type:drupal-library"
            ],
            "httpdocs/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "httpdocs/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "httpdocs/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "httpdocs/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "httpdocs/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
